.basic-select { font-size: 13px; line-height: 18px; font-weight: 500; display: block; color: var(--vscode-input-foreground); background: var(--vscode-input-background); font-family: var(--vscode-font-family); border: none; box-shadow: 0 0 0 1px var(--vscode-input-border) inset; border-radius: 3px; outline: 0px; padding: 5px 8px; min-height: 28px; box-sizing: border-box; }

